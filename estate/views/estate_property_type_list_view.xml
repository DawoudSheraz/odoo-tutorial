<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record
            id="estate_property_type_list_view"
            model="ir.ui.view"
    >
        <field name="model">estate.property_type</field>
        <field name="arch" type="xml">
            <list>
<!--   Sequence with handle widget allows Drag and drop of ordering of objects which changes the sequence-->
                <field name="sequence" widget="handle" />
                <field name="name" />
                <field name="offer_ids" />
                <field name="offer_count" />
            </list>
        </field>
    </record>
</odoo>